<launch>

    <arg name="wait_nodes" default=""/>
    <arg name="linear_scale" default="1" />
    <arg name="angular_scale" default="1" />

    <node pkg="webui-ros-joystick" type="node_server.sh" name="node_server" output="screen" args=" --wait $(arg wait_nodes) --lin $(arg linear_scale) --ang $(arg angular_scale)"/>

</launch>
